{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "declaration": true,
    "noResolve": false,
    "module": "commonjs",
    "target": "es5",
    "outDir": "dist",
    "lib": ["dom", "es5", "es2015"]
  },
  "typedocOptions": {
    "name": "MediaFire JavaScript SDK",
    "theme": "minimal",
    "mode": "file",
    "out": "docs",
    "includes": "src",
    "hideGenerator": true,
    "excludeProtected": true,
    "excludePrivate": true,
    "excludeNotExported": true,
    "toc": [
      "Device",
      "File",
      "Folder",
      "System",
      "User",
      "DeviceChanges",
      "DeviceForeignResources",
      "DeviceStatus",
      "DeviceTrash",
      "FileCopied",
      "FileDeleted",
      "FileInfo",
      "FileLinks",
      "FileMoved",
      "FilePurged",
      "FileRecentlyModified",
      "FileUpdated",
      "FileZip",
      "FolderConfiguredFiledrop",
      "FolderContent",
      "FolderCopied",
      "FolderDeleted",
      "FolderDepth",
      "FolderInfo",
      "FolderMoved",
      "FolderPurged",
      "FolderSearch",
      "FolderUpdated",
      "SystemLimits",
      "SystemStatus",
      "UserAvatar",
      "UserInfo",
      "UserRenewSession"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}